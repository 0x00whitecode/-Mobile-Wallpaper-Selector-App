{
  "expo": {
    "name": "wallpaperapp",
    "slug": "wallpaperapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wallpaperapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whitejeh.wallpaperapp"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png",
        "backgroundColor": "#E6F4FE"
      },
      "icon": "./assets/images/icon.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.white_jeh.wallpaperapp",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/font/ClashDisplay_Complete/Fonts/TTF/ClashDisplay-Variable.ttf",
            "./assets/font/Lemon,Plus_Jakarta_Sans,Poppins,Raleway/Lemon/Lemon-Regular.ttf",
            "./assets/font/Lemon,Plus_Jakarta_Sans,Poppins,Raleway/Poppins/Poppins-Regular.ttf",
            "./assets/font/Lemon,Plus_Jakarta_Sans,Poppins,Raleway/Poppins/Poppins-Bold.ttf"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcce5c9c-d7f5-4605-b553-ae1981a96e80"
      }
    },

    "owner": "white_jeh"
  }
}
