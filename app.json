{
  "expo": {
    "name": "wallpaperapp",
    "slug": "wallpaperapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wallpaperapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.white-jeh.wallpaperapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.white_jeh.wallpaperapp",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "windows": {
      "package": "com.whitejeh.wallpaperapp",
      "certificateThumbprint": "",
      "publisherDisplayName": "Wallpaper Studio"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/font/ClashDisplay_Complete/ClashDisplay-Regular.ttf",
            "./assets/font/ClashDisplay_Complete/ClashDisplay-Bold.ttf",
            "./assets/font/ClashDisplay_Complete/ClashDisplay-Medium.ttf",
            "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Lemon-Regular.ttf",
            "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/PlusJakartaSans-Regular.ttf",
            "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Poppins-Regular.ttf",
            "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Raleway-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "ClashDisplay",
                "fontDefinitions": [
                  {
                    "path": "./assets/font/ClashDisplay_Complete/ClashDisplay-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/font/ClashDisplay_Complete/ClashDisplay-Bold.ttf",
                    "weight": 700
                  }
                ]
              },
              {
                "fontFamily": "Poppins",
                "fontDefinitions": [
                  {
                    "path": "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Poppins-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Poppins-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/font/ClashDisplay_Complete/ClashDisplay-Regular.ttf",
              "./assets/font/ClashDisplay_Complete/ClashDisplay-Bold.ttf",
              "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Poppins-Regular.ttf",
              "./assets/font/Lemon_Plus_Jakarta_Sans_Poppins_Raleway/Poppins-Bold.ttf"
            ]
          }
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
